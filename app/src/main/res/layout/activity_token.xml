<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TokenActivity">

    <EditText
        android:id="@+id/tokenValue"
        android:layout_width="314dp"
        android:layout_height="70dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="81dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="150dp"
        android:ems="10"
        android:hint="Type Token"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newtokenEnter" />

    <Button
        android:id="@+id/tokenEnter"
        android:layout_width="303dp"
        android:layout_height="80dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="73dp"
        android:backgroundTint="#2196F3"
        android:text="FInd List from Token"
        android:textAlignment="center"
        android:textColor="#2C397F"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newtokenValue" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="458dp"
        android:layout_height="78dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="39dp"
        android:background="#2196F3"
        android:backgroundTint="#002196F3"
        android:backgroundTintMode="add"
        android:text="List App"
        android:textAlignment="center"
        android:textColor="#2C3980"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/logout"
        android:layout_width="96dp"
        android:layout_height="55dp"
        android:backgroundTint="#2196F3"
        android:backgroundTintMode="add"
        android:text="Logout"
        android:textAlignment="center"
        android:textColor="#2C397F"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/newtokenValue"
        android:layout_width="314dp"
        android:layout_height="70dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="13dp"
        android:ems="10"
        android:hint="Type Name of New List"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/newtokenEnter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/newtokenEnter"
        android:layout_width="303dp"
        android:layout_height="80dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="94dp"
        android:backgroundTint="#2196F3"
        android:text="Create new list"
        android:textAlignment="center"
        android:textColor="#2C397F"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/newtokenValue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newtokenValue" />

</androidx.constraintlayout.widget.ConstraintLayout>